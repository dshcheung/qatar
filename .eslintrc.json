{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "semi": [2, "never"],
    "comma-dangle": [2, "never"],
    "max-len": 0,
    "radix": 0,
    "global-require": 0,
    "prefer-object-spread": 0,
    "object-curly-newline": 0,
    "function-paren-newline": 0
  }
}
